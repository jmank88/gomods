import (
	"example.com/a/c"
	"example.com/b"
)

var (
	_ = b.Foo
	_ = c.Baz
)

-- a/go.mod --
module example.com/a

go 1.21.3

replace (
	example.com/a/c => ./c
	example.com/b => ../b
)

require (
	example.com/a/c v0.0.0-00010101000000-000000000000
	example.com/b v0.0.0-00010101000000-000000000000
)

-- a/c/c.go --
